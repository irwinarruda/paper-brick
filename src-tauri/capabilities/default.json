{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:webview:allow-create-webview-window",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:window:allow-set-size",
    "core:window:allow-set-position",
    "core:app:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "fs:default",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$HOME/**"
        },
        {
          "path": "$APPDATA/*"
        },
        {
          "path": "$APP/**"
        }
      ]
    },
    "shell:allow-open",
    "dialog:allow-message",
    "dialog:default",
    "dialog:allow-open",
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname"
  ]
}
